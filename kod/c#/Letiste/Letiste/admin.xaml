        <Label Content="Vzdálenost" HorizontalAlignment="Left" Margin="10,250,0,0" VerticalAlignment="Top"/>
        <Label Content="Odlet" HorizontalAlignment="Left" Margin="10,300,0,0" VerticalAlignment="Top"/>
