        <TextBlock x:Name="Output" HorizontalAlignment="Left" Margin="10,411,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top"/>
        <ListBox x:Name="listik" HorizontalAlignment="center" Height="316" Margin="470,100,10,34" VerticalAlignment="center" Width="320" SelectionChanged="listik_SelectionChanged_1"/>




